@page
@model RazorPagesUI.AddAddressModel
@{
    ViewData["Title"] = "AddAddress";
}

<h1>AddAddress</h1>

<form method="post">
   <input type="text" asp-for="AddressInsert.StreetAddress" placeholder="Street Address" />
    <input type="text" asp-for="AddressInsert.City" placeholder="City" />
    <input type="text" asp-for="AddressInsert.State" placeholder="State" />
    <input type="text" asp-for="AddressInsert.ZipCode" placeholder="Zip Code" />
   <button type="submit" class="btn btn-primary">Submit</button>

</form>